<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>$ext_safeprojectname$</id>
		<version>1.0.0-beta1</version>
		<title>MvvmCross - $ext_pluginname$ Plugin</title>
		<authors>$username$</authors>
		<owners>$username$</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/MvvmCross/MvvmCross-Plugins</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>A $ext_pluginname$ plugin for MvvmCross</description>
		<tags>mvvm mvvmcross cross xamarin android ios forms monodroid monotouch xamarin.android xamarin.ios xamarin.forms wpf windows8 winrt net net45 netcore wp wpdev windowsphone windowsstore uwp plugin</tags>
		<iconUrl>http://i.imgur.com/BvdAtgT.png</iconUrl>
		<dependencies>
			<dependency id="MvvmCross.Platform" version="4.3.0" />
		</dependencies>
	</metadata>
	<files>

		<!-- PCL -->
		<file src="..\Release\Portable\$ext_safeprojectname$.*" target="lib\portable-net45+win+wpa81+wp80" />

		<!-- dotnet -->
		<file src="..\Release\Portable\$ext_safeprojectname$.*" target="lib\dotnet" />

		<!-- windows common -->
		<file src="..\Release\Portable\$ext_safeprojectname$.*" target="lib\win81" />
		<file src="..\Release\WindowsCommon\$ext_safeprojectname$.WindowsCommon.*" target="lib\win81" />
		<file src="BootstrapContent\$ext_pluginname$PluginBootstrap.cs.pp" target="content\win81\Bootstrap\$ext_pluginname$PluginBootstrap.cs.pp" />

		<!-- windows Phone 8.1 -->
		<file src="..\Release\Portable\$ext_safeprojectname$.*" target="lib\wpa81" />
		<file src="..\Release\WindowsCommon\$ext_safeprojectname$.WindowsCommon.*" target="lib\wpa81" />
		<file src="BootstrapContent\$ext_pluginname$PluginBootstrap.cs.pp" target="content\wpa81\Bootstrap\$ext_pluginname$PluginBootstrap.cs.pp" />

    <!-- uwp -->
    <file src="..\Release\Portable\$ext_safeprojectname$.*" target="lib\uap" />
    <file src="..\Release\UWP\$ext_safeprojectname$.UWP.*" target="lib\uap" />
    <file src="BootstrapContent\$ext_pluginname$PluginBootstrap.cs.pp" target="content\uap\Bootstrap\$ext_pluginname$PluginBootstrap.cs.pp" />

    <!-- droid -->
		<file src="..\Release\Portable\$ext_safeprojectname$.*" target="lib\MonoAndroid" />
		<file src="..\Release\Droid\$ext_safeprojectname$.Droid.*" target="lib\MonoAndroid" />
		<file src="BootstrapContent\$ext_pluginname$PluginBootstrap.cs.pp" target="content\MonoAndroid\Bootstrap\$ext_pluginname$PluginBootstrap.cs.pp" />

		<!-- iOS -->
		<file src="..\Release\Portable\$ext_safeprojectname$.*" target="lib\Xamarin.iOS10" />
		<file src="..\Release\iOS\$ext_safeprojectname$.iOS.*" target="lib\Xamarin.iOS10" />
		<file src="iOSBootstrapContent\$ext_pluginname$PluginBootstrap.cs.pp" target="content\Xamarin.iOS10\Bootstrap\$ext_pluginname$PluginBootstrap.cs.pp" />

	</files>
</package>
















